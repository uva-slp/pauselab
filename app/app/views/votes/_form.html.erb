 <h1> Vote </h1>
<h3>Pick your top three </h3>
<%= simple_form_for :votes do |f| %>

<%= f.input :proposal1, collection: @Proposals, label_method: :description %>

<%= f.input :proposal2, collection: @Proposals, label_method: :description %>


<%= f.input :proposal3, collection: @Proposals, label_method: :description %>

 <div class='form-group'>
      <%= f.button :submit, value: 'Submit', class: 'btn-primary' %>
    </div>
<% end %>

